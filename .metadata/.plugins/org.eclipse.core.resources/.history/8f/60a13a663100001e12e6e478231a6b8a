package Hello.com;

import java.util.Scanner;

public class HelloWorld {
	 public static Integer SumTwoNum(int a, int b) {
	    	int sum;
	    	sum = a+b;
	    	return sum;
	    }
    public static void main(String[] args) {
        //Viết chương trình Java để tính tổng hai số nguyên và in kết quả ra màn hình.
    	
    	System.out.println("Please input two nubmer integer");
    	Scanner sc = new Scanner(System.in);
    	System.out.print("a = ");
    	int a = sc.nextInt();
    	sc.nextLine();
    	System.out.print("b = ");
    	int b = sc.nextInt();  
    	System.out.println(SumTwoNum(a,b));
    }
   
}